#include <Servo.h>
Servo servo1;
Servo servo2;

void setup()
{
  servo1.attach(8);
  servo2.attach(9);
}
void loop()
{
  servo1.write(0);
  servo2.write(0);
  delay(1000);
  servo1.write(10);
  servo2.write(10);
  delay(1000);
  servo1.write(20);
  servo2.write(20);
  delay(1000);
  servo1.write(30);
  servo2.write(30);
  delay(1000);
  servo1.write(40);
  servo2.write(40);
  delay(1000);
  servo1.write(50);
  servo2.write(50);
  delay(1000);
  servo1.write(60);
  servo2.write(60);
  delay(1000);
  servo1.write(70);
  servo2.write(70);
  delay(1000);
  servo1.write(80);
  servo2.write(80);
  delay(1000);
  servo1.write(90);
  servo2.write(90);
  delay(1000);
  servo1.write(100);
  servo2.write(100);
  delay(1000);
  servo1.write(110);
  servo2.write(110);
  delay(1000);
  servo1.write(120);
  servo2.write(120);
  delay(1000);
  servo1.write(130);
  servo2.write(130);
  delay(1000);
  servo1.write(140);
  servo2.write(140);
  delay(1000);
  servo1.write(150);
  servo2.write(150);
  delay(1000);
  servo1.write(160);
  servo2.write(160);
  delay(1000);
  servo1.write(170);
  servo2.write(170);
  delay(1000);
  servo1.write(180);
  servo2.write(180);
  delay(1000);
  servo1.write(0);
  servo2.write(0);
  delay(5000);
}
